{
  "name": "example-adb-mle",
  "version": "1.0.0",
  "description": "Oracle Database MLE Logic for SneakerHeadz",
  "main": "src/sneaker_logic.js",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "build": "node scripts/build_mle.js",
    "deploy": "npm run build && node scripts/deploy_db.js",
    "deploy:local": "pwsh -File scripts/deploy_local.ps1",
    "test:unit": "vitest run tests/unit",
    "test:it": "vitest run tests/integration/sneaker_it.test.js"
  },
  "keywords": [
    "oracle",
    "mle",
    "javascript"
  ],
  "author": "SneakerHeadz Dev",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.56.0",
    "oracledb": "^6.10.0",
    "prettier": "^3.1.0",
    "testcontainers": "^11.10.0",
    "vitest": "^4.0.16"
  }
}